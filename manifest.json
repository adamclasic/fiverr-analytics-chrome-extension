{
   "action": {
      "default_icon": {
         "128": "icon128.png",
         "32": "icon32.png",
         "48": "icon48.png"
      },
      "default_popup": "popup.html",
      "default_title": "Fiverr Analytics Assistant"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "content_scripts": [ {
      "css": [ "style.css" ],
      "js": [ "assets/jquery-3.5.1.min.js", "logic.js" ],
      "matches": [ "https://www.fiverr.com/*", "https://www.fiverrpromotion.net/*" ]
   } ],
   "description": "Fiverr Analytics Assistant - View Buyer details when sending buyer request",
   "host_permissions": [ "https://www.fiverr.com/*", "https://www.fiverrpromotion.net/*" ],
   "key": "",
   "manifest_version": 3,
   "name": "Fiverr Analytics Assistant",
   "permissions": [ "storage" ],
   "short_name": "FSA",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.0",
   "web_accessible_resources": [ {
      "matches": [ "https://www.fiverr.com/*" ],
      "resources": [ "images/001-checkout.png", "images/002-shopping-bag.png", "images/001-clock.png", "images/002-placeholder.png", "images/003-heart.png", "images/001-alarm.png", "images/notification.png", "images/notification-tone.mp3", "images/favicon-32x32.png", "images/001-ranking.png", "images/002-promotion-1.png", "images/003-promotion.png", "images/tip-icon.png" ]
   } ]
}
